{
	"jsx": {
		"target": "esnext",
		"parser": {
			"syntax": "javascript",
			"jsx": true,
			"decorators": false,
			"dynamicImport": false
		},
		"transform": {
			"react": {
				"pragma": "React.createElement",
				"pragmaFrag": "React.Fragment",
				"throwIfNamespace": true,
				"useBuiltins": false,
				"runtime": "automatic"
			},
			"hidden": {
				"jest": true
			}
		}
	},
	"module": {
		"type": "commonjs",
		"strict": false,
		"strictMode": true,
		"lazy": false,
		"noInterop": false
	}
}
